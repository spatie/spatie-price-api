{
    "name": "spatie/spatie-price-api",
    "description": "The Price API used at promotional sites for our own products",
    "keywords": [
        "spatie",
        "spatie-price-api"
    ],
    "homepage": "https://github.com/spatie/spatie-price-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.16"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\PriceApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\PriceApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\PriceApi\\PriceApiServiceProvider"
            ],
            "aliases": {
                "PriceApi": "Spatie\\PriceApi\\PriceApiFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
